cod = str(input('Введите зашифрованный код:'))
alf = 'QWERTYUIOPASDFGHJKLZXCVBNM'
alf_ch = '1234567890'
ind1 = 0
ind2 = 0
k = 0
ind_num = 0
for s in cod:
    if  s in alf:
        ch_1 = s
        ind1 = cod.index(s)
        break
for s in cod:
    if s in alf_ch:
        ch_2 = s
        ind_num = cod.index(s)
        ind2 = ind_num+1
        break
step = ind2 - ind1
res = []
for i in range(ind1, len(cod), step):
    res.append(cod[i])
    if cod[i] == '.':
        break
print(''.join(res))